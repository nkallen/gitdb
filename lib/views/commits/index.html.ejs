<% include ../_head.html.ejs %>
<ul class="breadcrumb">
  <li><a href="<%= url.repo(repo) %>"><%= repo %></a></li>
  <li><a href="<%= url.refs(repo) %>">Refs</a></li>
  <li><a href="<%= url.ref(repo, ref) %>"><%= ref %></a></li>
  <li class="active">Commits</li>
</ul>
<div class="list-group">
<%
  for (var c in commits) {
    var commit = commits[c];
%>
<a href="<%= url.commit(repo, commit) %>" class="list-group-item">
  <dl class="dl-horizontal">
    <dt>commit</dt>
    <dd><%= commit %></dd>
    <dt>Message</dt>
    <dd><%= commit.message() %></dd>
    <dt>Author</dt>
    <dd><%= commit.author() %></dd>
    <dt>Date</dt>
    <dd><%= commit.date() %></dd>
  </dl>
</a>
<% } %>
</ol>
